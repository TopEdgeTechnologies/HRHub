                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Email)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Phone)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DesignationTitle)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ExperienceInYears)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Gender)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.City)
                                    </td> 
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CurrentCompany)
                                    </td>
                                    <td>
                                        @item.ApplyDate.Value.ToString("yyyy-MMM-dd")
                                        
                                    </td>
                                     <td>
                                        @Html.DisplayFor(modelItem => item.ExpectedSalary)
                                    </td>


