                                        <td class="text-end">
                                            <div class="d-flex">
                                                @if (Convert.ToBoolean(ViewBag.IsEdit))
                                                {
                                                    <a class="action-btns1" href="#" Onclick="loadModal(@item.CandidateId)" >
                                                        <i class="feather feather-check text-success mt-1">
                                                        </i>
                                                    </a>
