@model HRHUBAPI.Models.Candidate


<style>
    .modal-dialog {
        max-width: 1012px;
    }

    .dt-button.buttons-columnVisibility {
       
        color: grey;
    }

        .dt-button.buttons-columnVisibility.active {
            color: green;
        }
</style>

<input type="hidden" asp-for="CompanyId"/>
<input type="hidden" asp-for="UrlRequestSatausID" />
<!-- Row -->
<div class="row row-sm mt-2">

    <div class="col-lg-12">

        <div class="card">
            <div class="card-header">
                 @if(Model.UrlRequestSatausID==1){
                    <h3 class="card-title">Applied Candidate</h3>
                }
                else if (Model.UrlRequestSatausID == 2)
                {
                    <h3 class="card-title">Short Candidate List</h3>
                }
                else if (Model.UrlRequestSatausID == 3)
                {
                    <h3 class="card-title">Interview Candidate List</h3>
                }
                else if (Model.UrlRequestSatausID == 4)
                {
                    <h3 class="card-title">Re-Interview Candidate List</h3>
                }
               else if (Model.UrlRequestSatausID == 5)
                {
                    <h3 class="card-title">Qualified Candidate List</h3>
                }
                else if (Model.UrlRequestSatausID == 6)
                {
                    <h3 class="card-title">Not-Qualified Candidate List</h3>
                }
                else if (Model.UrlRequestSatausID == 7)
                {
                    <h3 class="card-title">Offered Candidate List</h3>
                }
                else if (Model.UrlRequestSatausID == 8)
                {
                    <h3 class="card-title">Approved Candidate List</h3>
                } 
                else if (Model.UrlRequestSatausID == 9)
                {
                    <h3 class="card-title">Rejected Candidate List</h3>
                }
                @if (Convert.ToBoolean(ViewBag.IsNew))
                {
                   
                        @*<a asp-action="CandidateCreateOrUpdate" asp-controller="Candidate" class="btn btn-primary"><i class="fe fe-plus-circle me-1"></i>Add New </a>*@
                    <div class="card-options">
                        <div class="btn-list">
                            @if(Model.UrlRequestSatausID==1)
                            {
                                 <a asp-action="CandidateCreateOrUpdate" asp-controller="Candidate" class="btn btn-primary"><i class="fe fe-plus-circle me-1"></i>Add Candidates </a>
                                
                            }

