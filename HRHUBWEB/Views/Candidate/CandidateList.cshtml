                                <th class="border-bottom-0">Current Company</th>
                                <th class="border-bottom-0">Applied Date</th>
                                <th class="border-bottom-0">Expected Salary</th>
                                
                                <th class=""style="max-width: 150px; word-wrap: break-word!important; overflow: hidden;">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                int count = 1;
                            }
